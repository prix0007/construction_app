extends layout

block content
    .jumbotron.background-image-aboutUs.text-white.text-right
        h1.display-4 Contact Us
        p.lead
            | Keep in touch with us.
        hr.my-4
        p
            | Lorem ipsum dolor sit amet consectetur adipisicing elit. Deleniti deserunt placeat vel magni, quo, doloremque error sint ex ratione debitis dicta. Maxime nam, aperiam ex obcaecati delectus ut molestiae perspiciatis?
    br
    .row
        .col-md-3
            .card
                .card-header    
                    h5.text-center.font-weight-bold Contact Us
                .card-body
                    nav.nav.flex-column
                        a.nav-link.active.text-black(href='/navigation/contact-enquiry') Enquiry Form
                        a.nav-link.secondary(href='/navigation/contact-feedback') Feedback Form
            hr
            h6.text-right Ongoing Projects
            #carouselExampleControls.carousel.slide(data-ride='carousel')
                .carousel-inner
                    .carousel-item.active
                        img.d-block.w-100(src='/img/photos/image1.jpg', alt='...')
                    .carousel-item
                        img.d-block.w-100(src='/img/photos/image3.jpg', alt='...')
                    .carousel-item
                        img.d-block.w-100(src='/img/photos/image4.jpg', alt='...')
                a.carousel-control-prev(href='#carouselExampleControls', role='button', data-slide='prev')
                    span.carousel-control-prev-icon(aria-hidden='true')
                    span.sr-only Previous
                a.carousel-control-next(href='#carouselExampleControls', role='button', data-slide='next')
                    span.carousel-control-next-icon(aria-hidden='true')
                    span.sr-only Next
        .col-md-9
            nav(aria-label='breadcrumb')
                ol.breadcrumb
                    li.breadcrumb-item
                        a(href='/') Home
                    li.breadcrumb-item.active(aria-current='page') Contact Us
            .card.text-left
                .card-header
                    h3 Contact Us
                .card-body
                    h5.text-black Visit us at : 
                    p.p-0.m-0 FORTUNE INFRA PROPERTIES PVT. LTD.
                    | Mount Poonamallee Road, Manapakkam, P.B.No.979, Chennai – 600 089.
                    br
                    br
                    h5 Timings: 
                    p.p-0.m-0 10:00AM - 8:00PM 
                    | Monday - Friday
    br 
    .row
        .col-md-6
            ul.list-group.list-group-flush
                li.list-group-item.font-weight-bold 
                    h3 Contact Us
                li.list-group-item 
                    img(src="/img/logo/location.svg" class="small-icon" alt="Location")
                    | Mount Poonamallee Road, Manapakkam, P.B.No.979, Chennai – 600 089.
                li.list-group-item 
                    img(src="/img/logo/call.svg" class="small-icon" alt="Call Mobile")
                    | 91-44-22526000, 22528000, Fax:+91-44-2249 3317
                li.list-group-item
                    img(src="/img/logo/email.svg" class="small-icon" alt="Email") 
                    | info@lntecc.com
        .col-md-6
            .card.bg-dark.text-black.h-100
                iframe.card-img(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2212.342475356946!2d79.05357640294034!3d21.16426581096249!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bd4c052e5bc862b%3A0x8b4fa6012f8f4f6f!2sIndian%20Institute%20of%20Information%20Technology%2CNagpur!5e0!3m2!1sen!2sin!4v1583642215557!5m2!1sen!2sin" width="auto" height="100%" frameborder="0" style="border:0;" allowfullscreen="")
            
        